{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "letsencryptapi": {
            "matchCondition": {
                "route": "/.well-known/acme-challenge/{code}"
            },
            "backendUri": "https://localhost/api/letsencrypt/.well-known/acme-challenge/{code}"
        },
        "api": {
            "matchCondition": {
                "route": "/api/{*restOfPath}"
            },
            "backendUri": "https://localhost/api/{restOfPath}"
        },
        "spa": {
            "matchCondition": {
                "route": "/{*restOfPath}"
            },
            "backendUri": "https://cwserverlessmsorders.z6.web.core.windows.net/{restOfPath}"
        }
    }
}